
@model List<EduTech.Models.Course>
<h1>Danh sách khóa học</h1>

<table class="table">
    <thead>
        <tr>
            <th>Tên khóa học</th>
            <th>Miêu tả</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.Name</td>
                <td>@course.Description</td>
                <td>
                    <a>Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
